{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://mosam-web-api.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "github": {
    "silent": true
  }
} 